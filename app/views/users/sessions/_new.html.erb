<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
  <%= f.label "Email" %>
  <%= f.email_field :email %>

  <%= f.label "Senha" %>
  <%= f.password_field :password %>

  <% if devise_mapping.rememberable? %>
    <%= f.check_box :remember_me %> <%= f.label "Relembrar" %>
  <% end %>

  <%= f.submit "Entrar" %>
<% end %>

